<h1>
<strong><%= identifier.capitalize %></strong>
</h1>
<h3>Url Breakdown</h3>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Frequency: </th>
          <th>Url: </th>
        </tr>
        <tbody>
        <% sorted_urls.each do |url| %>
          <tr>
            <td><%= url[1] %></td>
            <td><%= url[0] %></td>
          </tr>
        <% end %>
        </tbody>
      </thead>
    </table>
  </div>
</div>


<h3>Browser Breakdown</h3>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Frequency: </th>
          <th>Browser: </th>
        </tr>
        <tbody>
          <% web_browser_breakdown.each do |browser| %>
          <tr>
            <td><%= browser[1] %></td>
            <td><%= browser[0] %></td>
          </tr>
          <% end %>
        </tbody>
      </thead>
    </table>
  </div>
</div>

<h3>Operating System Breakdown</h3>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Frequency: </th>
          <th>OS: </th>
        </tr>
        <tbody>
          <% os_browser_breakdown.each do |browser| %>
          <tr>
            <td><%= browser[1] %></td>
            <td><%= browser[0] %></td>
          </tr>
          <% end %>
        </tbody>
      </thead>
    </table>
  </div>
</div>
